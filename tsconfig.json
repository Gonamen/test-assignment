{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "sourceMap": true
  },
  "include": ["src/**/*", "svg.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
